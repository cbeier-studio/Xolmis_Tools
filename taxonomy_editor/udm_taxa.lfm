    SQL.Strings = (
      'SELECT z.*,'
      '    u.full_name AS parent_taxon_name,'
      '    v.full_name AS valid_name,'
      '    o.full_name AS order_name,'
      '    f.full_name AS family_name,'
      '    s.full_name AS subfamily_name,'
      '    n.full_name AS genero_name,'
      '    e.full_name AS species_name,'
      '    g.full_name AS subspecies_group_name'
      'FROM zoo_taxa AS z'
      'LEFT JOIN zoo_taxa AS u ON z.parent_taxon_id = u.taxon_id'
      'LEFT JOIN zoo_taxa AS v ON z.valid_id = v.taxon_id'
      'LEFT JOIN zoo_taxa AS o ON z.order_id = o.taxon_id'
      'LEFT JOIN zoo_taxa AS f ON z.family_id = f.taxon_id'
      'LEFT JOIN zoo_taxa AS s ON z.subfamily_id = s.taxon_id'
      'LEFT JOIN zoo_taxa AS n ON z.genus_id = n.taxon_id'
      'LEFT JOIN zoo_taxa AS e ON z.species_id = e.taxon_id'
      'LEFT JOIN zoo_taxa AS g ON z.subspecies_group_id = g.taxon_id'
      'WHERE (z.active_status = 1)'
      'ORDER BY z.full_name ASC'
    )
    InsertSQL.Strings = (
      'INSERT INTO zoo_taxa ('
      '    full_name,'
      '    authorship,'
      '    formatted_name,'
      '    quick_code,'
      '    rank_id,'
      '    parent_taxon_id,'
      '    iucn_status,'
      '    extinct,'
      '    extinction_year,'
      '    sort_num,'
      '    order_id,'
      '    family_id,'
      '    subfamily_id,'
      '    genus_id,'
      '    species_id,'
      '    subspecies_group_id,'
      '    incertae_sedis,'
      '    ebird_code,'
      '    distribution,'
      '    insert_date,'
      '    update_date)'
      'VALUES ('
      '    :full_name,'
      '    :authorship,'
      '    :formatted_name,'
      '    :quick_code,'
      '    :rank_id,'
      '    :parent_taxon_id,'
      '    :iucn_status,'
      '    :extinct,'
      '    :extinction_year,'
      '    :sort_num,'
      '    :order_id,'
      '    :family_id,'
